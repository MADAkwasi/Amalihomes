{
  "name": "@amalihomes/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "build": "npm run build-env && npx nx build amalihomes",
    "build-env": "npx envnext -o apps/amalihomes/src/app/environments -e ts",
    "build-env-dev": "npx envnext -p .env -o apps/amalihomes/src/app/environments -e ts -w",
    "build:ssr": "npx nx build amalihomes --configuration production",
    "postbuild:ssr": "chmod +x ./postbuild.sh && ./postbuild.sh",
    "lint": "npx nx lint amalihomes",
    "test": "npx jest",
    "test:coverage": "npx jest --ci --runInBand --coverage",
    "test:ci": "jest --ci --runInBand --detectOpenHandles --coverage --forceExit",
    "app": "sleep 2 && npx nx serve amalihomes",
    "amalihomes": "npm-run-all --parallel app build-env-dev",
    "amalihomes:ssr": "npm run build && node dist/apps/amalihomes/server/server.mjs",
    "gen": "bash .scripts/generator.sh",
    "prepare": "npx husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.5",
    "@angular/common": "^19.2.5",
    "@angular/compiler": "^19.2.5",
    "@angular/core": "^19.2.5",
    "@angular/forms": "^19.2.5",
    "@angular/platform-browser": "^19.2.5",
    "@angular/platform-browser-dynamic": "^19.2.5",
    "@angular/platform-server": "^19.2.5",
    "@angular/router": "^19.2.5",
    "@angular/ssr": "^19.2.5",
    "@ngrx/effects": "^19.0.1",
    "@ngrx/entity": "^19.0.1",
    "@ngrx/store": "^19.0.1",
    "@ngrx/store-devtools": "^19.0.1",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "lucide-angular": "^0.482.0",
    "rxjs": "~7.8.0",
    "storyblok-js-client": "^6.10.10",
    "tailwind-scrollbar-hide": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.5",
    "@angular-devkit/core": "^19.2.5",
    "@angular-devkit/schematics": "^19.2.5",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.5",
    "@angular/language-service": "^19.2.5",
    "@codigex/envnext": "^2.0.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "20.5.0",
    "@nx/cypress": "20.5.0",
    "@nx/eslint": "20.5.0",
    "@nx/eslint-plugin": "20.5.0",
    "@nx/jest": "20.5.0",
    "@nx/js": "20.5.0",
    "@nx/storybook": "20.5.0",
    "@nx/web": "20.5.0",
    "@nx/workspace": "20.5.0",
    "@schematics/angular": "^19.2.5",
    "@storybook/addon-essentials": "8.6.4",
    "@storybook/angular": "8.6.4",
    "@storybook/core-server": "8.6.4",
    "@storybook/jest": "^0.2.3",
    "@storybook/test-runner": "^0.19.0",
    "@storybook/testing-library": "^0.2.2",
    "@swc-node/register": "~1.9.1",
